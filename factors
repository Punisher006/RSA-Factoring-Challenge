import sys
import math

def factorize(n):
    """Find two prime factors of n."""
    # Check divisibility from 2 up to the square root of n
    for i in range(2, int(math.isqrt(n)) + 1):
        if n % i == 0:
            return i, n // i
    return None, None  # In case no factors are found

def main(filename):
    with open(filename, 'r') as file:
        n = int(file.read().strip())
    p, q = factorize(n)
    if p and q:
        print(f"{n}={p}*{q}")
    else:
        print(f"Failed to factorize {n}")

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: ./rsa <filename>")
        sys.exit(1)
    filename = sys.argv[1]
    main(filename)

